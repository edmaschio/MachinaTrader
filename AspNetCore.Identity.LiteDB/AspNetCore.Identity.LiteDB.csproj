<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/fabian-blum/AspNetCore.Identity.LiteDB</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/fabian-blum/AspNetCore.Identity.LiteDB/blob/master/LICENSE</PackageLicenseUrl>
    <Authors>Fabian Blum</Authors>
    <Company />
    <Description>A LiteDB provider for ASP.NET Core Identity framework</Description>
    <PackageTags>AspNetCore Identity LiteDB</PackageTags>
    <Version>2.6.0</Version>
    <AssemblyVersion>2.6.0.0</AssemblyVersion>
    <FileVersion>2.6.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LiteDB" Version="5.0.11" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="6.0.3" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>